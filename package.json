  "dependencies": {
    "axios": "^0.21.1",
    "chart.js": "^2.9.3",
    "chartjs": "^0.3.24",
    "concurrently": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.20.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.7.10",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.4",
    "react-calendar": "^2.19.2",
